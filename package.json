    "mock-jest-console": "^1.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "resources": "usable"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
